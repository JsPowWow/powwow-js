{
  "name": "powwow",
  "version": "0.0.1",
  "description": "A set of JavaScript utilities",
  "author": "Oleksandr Radvanskyi",
  "license": "Apache-2.0",
  "homepage": "https://github.com/JsPowWow/powwow-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JsPowWow/powwow-js.git"
  },
  "keywords": [
    "powwow"
  ],
  "bugs": {
    "url": "https://github.com/JsPowWow/powwow-js/issues"
  },
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "cleanup": "rimraf -rf node_modules",
    "clean:string-utils": "npm run clean --prefix tmp/string-utils",
    "clean:string-matches": "npm run clean --prefix tmp/string-matches",
    "build:string-utils": "npm run build --prefix tmp/string-utils",
    "build:string-matches": "npm run build --prefix tmp/string-matches",
    "clean:all": "npm run clean:types & npm run clean:core & npm run clean:string-matches",
    "build:all": "npm run build:types & npm run build:core & npm run build:string-matches"
  },
  "engines": {
    "npm": ">=7.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  }
}
